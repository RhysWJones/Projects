<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <from-view-id>/register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>registered</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loggedIn</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/register.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>View Cinemas</from-outcome>
            <to-view-id>/viewCinemas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>loggedOff</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Update Account</from-outcome>
            <to-view-id>/updateAccount.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Display Films</from-outcome>
            <to-view-id>/deleteFilm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Add Film</from-outcome>
            <to-view-id>/addFilm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>View Bookings</from-outcome>
            <to-view-id>/viewBookings.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/viewCinemas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>View Showings</from-outcome>
            <to-view-id>/viewShowings.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>loggedOff</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Back</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Display Films</from-outcome>
            <to-view-id>/selectFilm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/updateAccount.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Updated</from-outcome>
            <to-view-id>/updateAccount.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/viewShowings.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>loggedOff</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Book Tickets</from-outcome>
            <to-view-id>/bookTickets.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Back</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/bookTickets.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/viewShowings.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>loggedOff</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Tickets Booked</from-outcome>
            <to-view-id>/bookTickets.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/deleteFilm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Deleted</from-outcome>
            <to-view-id>/deleteFilm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Back</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/addFilm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Film Added</from-outcome>
            <to-view-id>/addFilm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Back</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/viewBookings.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Back</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>LoggedOff</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Cancelled</from-outcome>
            <to-view-id>/viewBookings.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/selectFilm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Back</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>LoggedOff</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Film Selected</from-outcome>
            <to-view-id>/addShowing.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/addShowing.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Back</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>LoggedOff</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Showing Created</from-outcome>
            <to-view-id>/addShowing.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
