<Window x:Class="WPFDataParallelismProj.FolderPathWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFDataParallelismProj"
        mc:Ignorable="d"
        Title="Folder Path" Height="144.229" Width="539.131">
    <Grid Margin="0,0,12,11">
        <Label x:Name="Folder_Path_Label" Content="Choose file location for Store Data." HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Folder_Path_TextBox" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="414"/>
        <Button x:Name="Browse_Button" Content="Browse" HorizontalAlignment="Left" Margin="432,41,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="Browse_Button_Click"/>
        <Button x:Name="Submit_Button" Content="Submit" HorizontalAlignment="Left" Margin="432,69,0,0" VerticalAlignment="Top" Width="75" Height="21" Click="Submit_Button_Click"/>

    </Grid>
</Window>
